{
  "name": "class-dev-backend",
  "version": "1.0.0",
  "description": "class-dev backend application",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./build/src/server.js",
    "dev:start": "NODE_ENV=development nodemon ./src/server.ts",
    "migrate:dev:roles": "NODE_ENV=development ts-node ./src/roleSchemes/index.ts",
    "migrate:prod:roles": "NODE_ENV=production node ./build/src/roleSchemes/index.js",
    "migrate:test:roles": "NODE_ENV=testing ts-node ./src/roleSchemes/index.ts",
    "prestart": "npm run migrate:prod:roles",
    "predev": "npm run migrate:dev:roles",
    "pretest": "npm run migrate:test:roles",
    "env": "ts-node ./src/env.ts",
    "dev": "NODE_ENV=development nodemon",
    "lint": "eslint .  --ext .ts",
    "lint:fix": "eslint .  --ext .ts --fix",
    "build": "rimraf ./dist && rimraf ./build &&  gulp build",
    "test": "jest --coverage",
    "docs": "ts-node ./src/utils/docs/swagger.ts"
  },
  "keywords": [
    "appointment",
    "users",
    "timezones",
    "auth",
    "business",
    "product management",
    "client"
